<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="395.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eurobank.proyectoaplicacionesdeescritorio.controlador.MenuController">
   <children>
      <Label fx:id="labelTitulo" layoutX="158.0" layoutY="14.0" text="Eurobank">
