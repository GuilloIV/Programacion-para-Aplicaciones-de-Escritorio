            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Button fx:id="botonAdministrarSucursales" layoutX="110.0" layoutY="161.0" mnemonicParsing="false" onAction="#abrirVentanaAdministrarSucursales" prefHeight="25.0" prefWidth="177.0" text="Sucursales" />
      <Button fx:id="botonAdministrarCuentasBancarias" layoutX="110.0" layoutY="209.0" mnemonicParsing="false" onAction="#abrirVentanaAdministrarCuentasBancarias" prefHeight="25.0" prefWidth="177.0" text="Cuentas Bancarias" />
