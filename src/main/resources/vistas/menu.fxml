      <Label fx:id="labelAdministrar" layoutX="168.0" layoutY="136.0" text="Administrar" />
      <Label fx:id="labelCerrarSesion" layoutX="310.0" layoutY="377.0" onMouseClicked="#cerrarSesion" text="Cerrar sesión" textFill="#0095ff" underline="true">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
      <Label fx:id="labelBienvenido" layoutX="28.0" layoutY="82.0" text="Bienvenido:" />
      <Label fx:id="labelPuesto" layoutX="28.0" layoutY="107.0" text="Puesto:" />
      <Label fx:id="textNombre" layoutX="96.0" layoutY="82.0" text="nombre" />
      <Label fx:id="textPuesto" layoutX="96.0" layoutY="107.0" text="puesto" />
